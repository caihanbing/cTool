{
    "manifest_version": 3,
    "name": "Curl Request Tool",
    "version": "1.0",
    "permissions": [
        "storage",
        "downloads",
        "clipboardRead"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background/background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    }
}